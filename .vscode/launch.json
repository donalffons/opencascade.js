  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Tests",
      "request": "launch",
      "runtimeArgs": [
        "test"
      ],
      "runtimeExecutable": "yarn",
      "runtimeVersion": "14.12.0",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/src"
    }
  ]
}